type cat: car
type car: integer

type yeah: <
    member1: cat,
    member2: pointer to yeah,
    member3: car
>

program tugas_keamanan_data
kamus
    kucing: cat
    koko: car
    yogi: yeah
algoritma 
    kucing <- 100
    koko <- kucing*100 -1
    yogi.member1 <- koko-kucing
    yogi.member3 <- yogi.member1*2
    output(kucing, koko, yogi.member1, yogi.member3)
    return 1
endprogram
