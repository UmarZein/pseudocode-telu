function P1(n: integer) -> integer
kamus 
    t,k: integer
algoritma 
    t<-0 
    k<-1 
    while k<=n do 
        t <- t + 2*k-1
        k <- k + 1
    endwhile 
    return t
endfunction

function P2(n: integer) -> integer 
algoritma 
    return n*n 
endfunction

program tugas_keamanan_data
kamus
    n, result: integer
algoritma 
    input(n)

    result <- P2(n)

    output(result)
    return 1
endprogram
