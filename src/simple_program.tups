function P1(n: integer) -> integer
kamus 
    t,k: integer
algoritma 
    t<-0 
    k<-1 
    while k<=n do 
        t <- t + 2*k-1
        k <- k + 1
    endwhile 
    return t
endfunction

function P2(n: integer) -> integer 
algoritma 
    return n*n 
endfunction

program tugas_keamanan_data
kamus
    n, p1_n, p2_n: integer
algoritma 
    n<-100
    p1_n <- P1(n)
    p2_n <- P2(n)

    output(p1_n,p2_n)
    return 1
endprogram
