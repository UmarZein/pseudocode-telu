function random() -> integer
kamus 
    i,j,n: integer 
algoritma 
    i<-0
    j<-0
    n<-1
    while i<4 do 
        j <- j + rand()*n
        n <- n*32768
        i<-i+1 
    endwhile
    return j
endfunction

